<p id="notice"><%= notice %></p>

<h1>Dataloaders</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Mcategory</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dataloaders.each do |dataloader| %>
      <tr>
        <td><%= dataloader.name %></td>
        <td><%= dataloader.mcategory_id %></td>
        <td><%= dataloader.name %></td>
        <td><%= dataloader.document_file_name %></td>
        <td><%= link_to 'Import', home_index18_path(:dataloader_id => dataloader.id) %></td>
        <td>
          <%= link_to(dataloader.document.url, target: "_blank") do %>
            <i class="btn btn-primary glyphicon glyphicon-cloud-download"></i>
          <% end %>
        </td>
        <td><%= link_to 'Show', dataloader %></td>
        <td><%= link_to 'Edit', edit_dataloader_path(dataloader) %></td>
        <td><%= link_to 'Destroy', dataloader, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dataloader', new_dataloader_path %>
