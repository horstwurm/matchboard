    <% I18n.locale = :de %>

<!DOCTYPE html>
<html>
  <head>
    <% if true %>
    <title>TGCloud Plattform</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
    <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAP0PoTXZAKRi5IO3fM5yRO39puZPiR0iw"></script>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <% end %>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Baker Bootstrap Theme</title>
    <meta name="description" content="Free Bootstrap Theme by BootstrapMade.com">
    <meta name="keywords" content="free website templates, free bootstrap themes, free template, free bootstrap, free website template">

    <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:400,300|Raleway:300,400,900,700italic,700,300,600">

  </head>
  
  <body>

  <div class="loader"></div>
  <div id="myDiv">

    <%= render 'layouts/header' %>

    <%= render 'layouts/feature' %>

    <%= render 'layouts/yield' %>

    <%= render 'layouts/service' %>

    <%= render 'layouts/portfolio' %>

    <%= render 'layouts/testimonial' %>

    <%= render 'layouts/blog' %>

    <%= render 'layouts/contact' %>

    <% flash.each do |name, msg| %>
      <% content_tag(:div, msg, class:"alert alert-warning") %>
      <div id="snackbar"><%= msg %></div>
    <% end %>

  </div>


  <% if false %>
    <%= render 'layouts/header' %>
    
    <div class='container-fluid testcontainer'>
        <% flash.each do |name, msg| %>
          <% content_tag(:div, msg, class:"alert alert-warning") %>
          <div id="snackbar"><%= msg %></div>
        <% end %>

        <!--<% render 'layouts/advertise' %>-->

        <%= yield %>

    </div>
    <%= render 'layouts/footer' %>
    
  <% end %>

  </body>
</html>

<link rel="stylesheet" type="text/css" href="/theme_baker/css/jquery.bxslider.css">
